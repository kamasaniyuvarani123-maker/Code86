#include <stdio.h>
int main(void) {
    char s1[100], s2[100];
    printf("Enter first string: ");
    gets(s1);
    printf("Enter second string: ");
    gets(s2);   
int i = 0;
    while (s1[i] && s2[i] && s1[i] == s2[i])
        ++i;
 if (s1[i] == '\0' && s2[i] == '\0')
        printf("Strings are equal.\n");
    else
        printf("Strings differ.\n");
  return 0;
}
